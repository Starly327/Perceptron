    "import tkinter as tk\n",
    "from pylab import rand,plot,show,norm\n",
    "from tkinter import filedialog,dialog\n",
    "import os\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "#from matplotlib.figure import Figure\n",
    "\n",
    "\n",
    "window = tk.Tk()\n",
    "window.title('Perceptron')\n",
    "window.geometry('800x600')\n",
    "window.configure(background='white')\n",
    "file_path = ''\n",
    "fig = plt.figure(figsize=(5, 4), dpi=100)\n",
    "fig.add_subplot(111)\n",
    "\n",
    "def open_file():#選取.txt檔案\n",
    "    global file_path\n",
    "    global file_text\n",
    "    global fig,text1\n",
    "    text1.delete(1.0,\"end\")\n",
    "    fig.clear()\n",
    "    file_path = filedialog.askopenfilename(title=u'選擇檔案',initialdir=(os.path.expanduser('H:/')))\n",
    "    print('開啟檔案：',file_path)\n",
    "    Perceptron.main()#執行主程式\n",
    "\n",
    "\n",
    "class Perceptron:\n",
    "        def __init__(self):\n",
    "            global learning_entry\n",
    "            self.w = rand(2)*2-1\n",
    "            self.learningRate = float(learning_entry.get())\n",
    "\n",
    "        def response(self,x,label1,label2):#辨別tag\n",
    "            y = float(x[0])*self.w[0]+float(x[1])*self.w[1]\n",
    "            if y >= 0:\n",
    "                return label1\n",
    "            else:\n",
    "                return label2\n",
    "\n",
    "        def updateWeights(self,x,iterError):#更新鍵值權重\n",
    "            self.w[0] += self.learningRate*iterError*float(x[0])\n",
    "            self.w[1] += self.learningRate*iterError*float(x[1])\n",
    "            \n",
    "        def train(self,data,label1,label2):#-------------------------training訓練集-------------------------------\n",
    "            global weight_entry\n",
    "            learned = False\n",
    "            iteration = 0\n",
    "            globalError = 0.0\n",
    "            while not learned:\n",
    "                \n",
    "                error = 0\n",
    "                for x in data:\n",
    "                    r = self.response(x,label1,label2)#辨別訓練集tag\n",
    "                    if float(x[2]) != r:\n",
    "                        error += 1\n",
    "                        iterError = r - float(x[2])\n",//
    "                        self.updateWeights(x,iterError)#更新鍵值權重\n",
    "                        globalError += abs(iterError)\n",
    "                        \n",
    "                iteration += 1\n",
    "                if iteration > float(weight_entry.get()):\n",
    "                    text1.insert('insert',str(self.w[0])+' ')\n",
    "                    text1.insert('insert',str(self.w[1])+'\\n')\n",
    "                    text1.insert('insert','訓練辨識率:')\n",
    "                    text1.insert('insert',str((len(data)-error)/len(data))+'\\n')\n",
    "                    learned = True#---------------------------------------------------------------------------\n",
    "                    \n",
    "        @staticmethod\n",
    "        def generateData(n,label1,label2):#產生隨機1000個訓練集\n",
    "            xb = np.random.uniform(low=-5, high=5, size=(n,))\n",
    "            yb = np.random.uniform(low=-5, high=5, size=(n,))\n",
    "            inputs = []\n",
    "            for i in range(len(xb)):\n",
    "                if i % 2 == 0:\n",
    "                    inputs.append([xb[i],yb[i],label1])\n",
    "                else:\n",
    "                    inputs.append([xb[i],yb[i],label2])\n",
    "            return inputs\n",
    "\n",
    "        @staticmethod\n",
    "        def main():#-----------------------------------main--------------------------------------------\n",
    "            global file_path\n",
    "            global fig\n",
    "            canvas = FigureCanvasTkAgg(fig, master=window)\n",
    "            canvas.draw()\n",
    "            canvas.get_tk_widget().pack(side=tkinter.TOP, fill=tkinter.BOTH, expand=1)\n",
    "           \n",
    "            f = open(file_path)\n",
    "            inputs = []\n",
    "            for line in f.readlines():#讀測試集檔\n",
    "                line = line.strip()\n",
    "                x = line.split(' ')\n",
    "                inputs.append(x)\n",
    "            testset = np.array(inputs)\n",
    "            \n",
    "            count = 0\n",
    "            for x in testset: #辨別測試集tag\n",
    "                if count == 0:\n",
    "                    label1 = x[2]\n",
    "                count += 1\n",
    "                label2 = x[2]\n",
    "                if label2 != label1:\n",
    "                    break\n",
    "                 \n",
    "            trainset = Perceptron.generateData(1000,float(label1),float(label2))#產生隨機1000個訓練集\n",
    "            perceptron = Perceptron()\n",
    "            perceptron.train(trainset,float(label1),float(label2))#training訓練集\n",
    "            \n",
    "            correct = 0\n",
    "            for x in testset:#判斷測試集辨識率\n",
    "                r = perceptron.response(x,float(label1),float(label2))#辨別測試集tag\n",
    "                if r == float(x[2]):\n",
    "                    correct += 1\n",
    "                if x[2] == label1:\n",
    "                    plt.scatter(float(x[0]),float(x[1]),label= \"stars\", color= \"green\",marker= \"*\", s=30)\n",
    "                else:\n",
    "                    plt.scatter(float(x[0]),float(x[1]),label= \"stars\", color= \"red\",marker= \"*\", s=30)\n",
    "            text1.insert('insert','測試辨識率：')\n",
    "            text1.insert('insert',str(correct/len(testset))+'\\n')\n",
    "            text1.insert('insert','(需放大縮小視窗更新圖形)')\n",
    "            n = norm(perceptron.w)\n",
    "            ww = perceptron.w/n\n",
    "            ww1 = [ww[1], -ww[0]]\n",
    "            ww2 = [-ww[1], ww[0]]\n",
    "            plt.plot([ww1[0], ww2[0]],[ww1[1], ww2[1]],'k--')#------------------------------------------------------\n",
    "            \n",
    "def _quit():# stop mainloop()\n",
    "    window.quit()     \n",
    "    window.destroy()     \n",
    "\n",
    "#以下為主要GUI畫面配置\n",
    "learning_frame = tk.Frame(window)\n",
    "learning_frame.pack(side=tk.TOP)\n",
    "learning_label = tk.Label(learning_frame, text='學習率：')\n",
    "learning_label.pack(side=tk.LEFT)\n",
    "learning_entry = tk.Entry(learning_frame)\n",
    "learning_entry.pack(side=tk.LEFT)\n",
    "\n",
    "weight_frame = tk.Frame(window)\n",
    "weight_frame.pack(side=tk.TOP)\n",
    "weight_label = tk.Label(weight_frame, text='迭代次數：')\n",
    "weight_label.pack(side=tk.LEFT)\n",
    "weight_entry = tk.Entry(weight_frame)\n",
    "weight_entry.pack(side=tk.LEFT)\n",
    "\n",
    "result_label = tk.Label(window)\n",
    "result_label.pack()\n",
    "\n",
    "bt1 = tk.Button(window,text='開啟檔案',width=15,height=2,command=open_file)#點取按鈕執行open_file()\n",
    "bt1.pack()\n",
    "\n",
    "text1 = tk.Text(window,width=50,height=10,bg='white',font=('Arial',12))\n",
    "text1.pack()\n",
    "\n",
    "button = tk.Button(master=window, text=\"Quit\", command=_quit)\n",
    "button.pack(side=tk.BOTTOM)\n",
    "\n",
    "window.mainloop()"
   ]
